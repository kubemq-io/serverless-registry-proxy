{
  "name": "registry-proxy",
  "env": {
    "REGISTRY_HOST": {
      "value": "europe-docker.pkg.dev",
      "description": "hostname for target registry"
    },
    "REPO_PREFIX": {
      "description": "prefix for your images in the target registry (i.e. GCP project ID)",
      "value": "kubemq/images"
    }
  },
  "options": {
    "allow-unauthenticated": true
  }
}
